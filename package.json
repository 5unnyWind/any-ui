{
  "name": "@any-ui/core",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "unpkg": "dist/index.min.js",
  "scripts": {
    "start": "pnpm run dev",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "build": "pnpm build:clear && pnpm build:rollup && pnpm build:babel && pnpm build:types",
    "build:rollup": "rollup -c build_config/rollup.config.js --bundleConfigAsCjs",
    "build:babel": "babel --no-babelrc --config-file ./build_config/babel.config.js --extensions \".js,.ts,.tsx\" ./components --out-dir ./esm --ignore \"**/__tests__/**/*,**/*.d.ts\"",
    "build:types": "ttsc -p ./build_config & ttsc -p ./build_config --outDir ./esm",
    "preinstall": "npx -y only-allow pnpm",
    "build:clear": "rm -rf ./dist ./esm",
    "build:webpack": "webpack --progress",
    "test": "jest --watch ",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint:style": "stylelint --fix \"src/**/*.{css,scss}\"",
    "release": "pnpm build && pnpm publish --access public --non-interactive"
  },
  "license": "MIT",
  "description": "An open-source design agile approach for building modern web applications.",
  "homepage": "https://any-ui.ncuos.com/",
  "bugs": {
    "url": "https://github.com/5unnyWind/any-ui/issues/new"
  },
  "packageManager": "pnpm@7.27.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/5unnyWind/any-ui"
  },
  "keywords": [
    "any",
    "any ui",
    "components",
    "react components",
    "react ui"
  ],
  "files": [
    "/dist",
    "/esm"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.20.7",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "lodash": "^4.17.21",
    "lodash.chunk": "^4.2.0",
    "node-sass": "^7.0.3",
