{
  "name": "any-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "pnpm run dev",
    "dev": "dumi dev",
    "build": "father build",
    "build:watch": "father dev",
    "docs:build": "dumi build",
    "preinstall": "npx -y only-allow pnpm",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.dev.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "test": "jest",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint:style": "stylelint --fix \"src/**/*.{css,scss}\""
  },
  "packageManager": "pnpm@7.26.3",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.20.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "babel-jest": "^29.4.2",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-15": "^1.4.4",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.21",
    "node-sass": "^7.0.3",
    "rc-field-form": "^1.27.3",
    "rc-upload": "^4.3.4",
    "rc-util": "^5.27.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading": "^2.0.3",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "scroll-into-view-if-needed": "^3.0.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@craco/craco": "^7.0.0",
    "@ctrl/tinycolor": "^3.5.0",
    "@eslint/create-config": "^0.4.2",
    "@testing-library/react": "^13.4.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@umijs/lint": "^4.0.0",
    "craco-babel-loader": "^1.0.4",
    "dumi": "^2.0.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "father": "^4.1.0",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "jsdom": "^21.0.0",
    "lint-staged": "^13.1.0",
    "node-sass": "^7.0.3",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.2",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stylelint": "^14.16.1",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-prettier": "^2.0.0",
    "typedoc": "^0.23.24",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write ."
    ],
    "*.{json,scss,md}": "prettier --ignore-unknown --write"
  }
}
