{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": false,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "anyUI/es/*": ["components/*"],
      "anyUI/lib/*": ["components/*"],
      "anyUI/locale/*": ["components/locale/*"]
    },
  },
  "include": [
    "./src"
  ],
  "exclude": ["node_modules", "lib", "es"]
}
